                        const double*   mat_Q_for_K_sp,
                        const uint32_t* density_inds_for_K_ss,
                        const uint32_t* density_inds_for_K_sp,
                        const uint32_t  naos,
                        const double*   boys_func_table,
                        const double*   boys_func_ft)
