            if ((ik < pair_inds_count_for_K_pp) && (j < s_prim_count) && (l < p_prim_count * 3))
            {
                const auto i = pair_inds_i_for_K_pp[ik];
                const auto k = pair_inds_k_for_K_pp[ik];

