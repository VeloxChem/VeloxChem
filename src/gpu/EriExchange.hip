                                    + delta[a0][c0] * delta[a1][d0] * delta[b0][d1] * delta[b1][c1]
                                    + delta[a0][c0] * delta[a1][d1] * delta[b0][b1] * delta[c1][d0]
                                    + delta[a0][c0] * delta[a1][d1] * delta[b0][c1] * delta[b1][d0]
                                    + delta[a0][c0] * delta[a1][d1] * delta[b0][d0] * delta[b1][c1]
