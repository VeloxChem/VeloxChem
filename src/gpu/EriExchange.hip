                    const auto S1 = (a_i + a_j);
                    const auto S2 = (a_k + a_l);
                    const auto S4 = (a_i + a_j + a_k + a_l);

                    const double rij[3] = {x_j - x_i, y_j - y_i, z_j - z_i};
                    const double rkl[3] = {x_l - x_k, y_l - y_k, z_l - z_k};

