                                    + delta[a1][c1] * delta[a0][b0] * delta[b1][c0] * (PQ[d0] * QD_1 + PQ[d1] * QD_0)
                                    + delta[a1][c1] * delta[b0][c0] * delta[a0][b1] * (PQ[d0] * QD_1 + PQ[d1] * QD_0)
