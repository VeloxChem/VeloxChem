                    const auto y_k = d_prim_info[k / 6 + d_prim_count * 3];
                    const auto z_k = d_prim_info[k / 6 + d_prim_count * 4];

                    const auto a_l = s_prim_info[l_prim + s_prim_count * 0];
                    const auto c_l = s_prim_info[l_prim + s_prim_count * 1];
