                    const auto x_l = p_prim_info[l_prim / 3 + p_prim_count * 2];
                    const auto y_l = p_prim_info[l_prim / 3 + p_prim_count * 3];
                    const auto z_l = p_prim_info[l_prim / 3 + p_prim_count * 4];

