            if (skip_thread_block == m + 1) break;

            if ((threadIdx.y == 0) && (threadIdx.x == 0))
            {
                for (uint32_t y = 0; y < TILE_DIM_HALF; y++)
                {
                    for (uint32_t x = 0; x < TILE_DIM_HALF; x++)
