                    const auto S_kl_00 = c_k * c_l * pow(MATH_CONST_PI / (a_k + a_l), 1.5) * exp(-a_k * a_l / (a_k + a_l) * r2_kl);

                    const auto S1 = (a_i + a_j);
                    const auto S2 = (a_k + a_l);
