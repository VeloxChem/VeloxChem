                            + delta[a1][d0] * delta[b1][c0] * (PQ[a0] * PQ[b0] * PQ[d1] * (-1.0))
                            + delta[a1][b1] * delta[c0][d0] * (PQ[a0] * PQ[b0] * PQ[d1] * (-1.0))
                            + delta[a1][d0] * delta[b0][d1] * (PQ[a0] * PQ[b1] * PQ[c0] * (-1.0))
