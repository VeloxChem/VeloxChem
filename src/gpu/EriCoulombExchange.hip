            const auto PB_1 = (-a_i / (a_i + a_j)) * rij[b1];
            const auto QC_0 = (a_l / (a_k + a_l)) * rkl[c0];
            const auto QD_0 = (-a_k / (a_k + a_l)) * rkl[d0];
