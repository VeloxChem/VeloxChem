                            + delta[a0][d0] * delta[b0][c1] * (PQ[b1] * PQ[c0] * PQ[d1])
                            + delta[a0][b0] * delta[c1][d0] * (PQ[b1] * PQ[c0] * PQ[d1])
                            + delta[a0][c0] * delta[b0][d1] * (PQ[b1] * PQ[c1] * PQ[d0])
