                            + delta[a0][c0] * delta[b0][c1] * delta[a1][b1] * delta[d0][d1] * (-1.0)
                            + delta[a0][c0] * delta[b0][d0] * delta[a1][b1] * delta[c1][d1] * (-1.0)
                            + delta[a0][c0] * delta[b0][d1] * delta[a1][b1] * delta[c1][d0] * (-1.0)
