                            + delta[a0][c1] * delta[a1][d1] * (PB_0 * PB_1 * QD_0 * QC_0)
                            + delta[a0][d1] * delta[a1][c1] * (PB_0 * PB_1 * QD_0 * QC_0)
                            + delta[a0][c1] * delta[a1][d0] * (PB_0 * PB_1 * QD_1 * QC_0)
                            + delta[a0][d0] * delta[a1][c1] * (PB_0 * PB_1 * QD_1 * QC_0)
