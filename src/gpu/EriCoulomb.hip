                            + delta[d0][c1] * (QD_1 * QC_0)
                            + delta[d1][c0] * (QD_0 * QC_1)
                            + delta[d0][c0] * (QD_1 * QC_1)
                            + delta[c1][c0] * (QD_0 * QD_1)
                        )

