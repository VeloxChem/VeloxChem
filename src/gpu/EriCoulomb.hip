                            + delta[a0][c1] * delta[b1][d1] * (PQ[b0] * PQ[c0] * PQ[d0])
                            + delta[a0][d1] * delta[b1][c1] * (PQ[b0] * PQ[c0] * PQ[d0])
                            + delta[a0][b1] * delta[c1][d1] * (PQ[b0] * PQ[c0] * PQ[d0])
