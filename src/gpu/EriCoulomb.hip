                            + delta[b1][d0] * delta[c0][c1] * (PQ[a0] * PQ[b0] * PQ[d1])
                            + delta[b0][c1] * delta[d0][d1] * (PQ[a0] * PQ[b1] * PQ[c0])
                            + delta[b0][d0] * delta[c1][d1] * (PQ[a0] * PQ[b1] * PQ[c0])
