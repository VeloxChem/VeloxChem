                            + delta[a0][b1] * delta[c1][d0] * (PQ[b0] * PQ[c0] * PQ[d1])
                            + delta[a0][c0] * delta[b1][d1] * (PQ[b0] * PQ[c1] * PQ[d0])
                            + delta[a0][d1] * delta[b1][c0] * (PQ[b0] * PQ[c1] * PQ[d0])
